    viewStudentTransport.TblStudentTransport_StationExit AS TblStudentTransport_StationExit,
    viewStudentTransport.TblStudentTransport_Remark AS TblStudentTransport_Remark
FROM
    (((((((((((((((((PeoplePerson_{{ ConsumerAcronym }}.viewPerson
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudent ON ((viewPerson.TblPerson_Id = viewStudent.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentAgreement ON ((viewPerson.TblPerson_Id = viewStudentAgreement.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentBaptism ON ((viewPerson.TblPerson_Id = viewStudentBaptism.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentDisorder ON ((viewPerson.TblPerson_Id = viewStudentDisorder.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentFocus ON ((viewPerson.TblPerson_Id = viewStudentFocus.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentIntegration ON ((viewPerson.TblPerson_Id = viewStudentIntegration.TblStudent_serviceTblPerson)))
    LEFT JOIN PeoplePerson_{{ ConsumerAcronym }}.viewPerson viewPersonCoach ON ((viewStudentIntegration.TblStudentIntegration_serviceTblPerson = viewPersonCoach.TblPerson_Id)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentLiberation ON ((viewPerson.TblPerson_Id = viewStudentLiberation.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentLocker ON ((viewPerson.TblPerson_Id = viewStudentLocker.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentMedicalRecord ON ((viewPerson.TblPerson_Id = viewStudentMedicalRecord.TblStudent_serviceTblPerson)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentSubject ON ((viewPerson.TblPerson_Id = viewStudentSubject.TblStudent_serviceTblPerson)))
    LEFT JOIN EducationLessonSubject_{{ ConsumerAcronym }}.viewSubject ON ((viewStudentSubject.TblStudentSubject_serviceTblSubject = viewSubject.TblSubject_Id)))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentTransfer ON ((viewPerson.TblPerson_Id = viewStudentTransfer.TblStudent_serviceTblPerson)))
    LEFT JOIN CorporationCompany_{{ ConsumerAcronym }}.viewCompany ON ((viewStudentTransfer.TblStudentTransfer_serviceTblCompany = viewCompany.TblCompany_Id)))
    LEFT JOIN EducationSchoolType_{{ ConsumerAcronym }}.viewSchoolType viewSchoolTypeTransfer ON ((viewStudentTransfer.TblStudentTransfer_serviceTblType = viewSchoolTypeTransfer.TblType_Id)))
    LEFT JOIN EducationSchoolCourse_{{ ConsumerAcronym }}.tblCourse ON (((viewStudentTransfer.TblStudentTransfer_serviceTblCourse = EducationSchoolCourse_{{ ConsumerAcronym }}.tblCourse.Id)
        AND ISNULL(EducationSchoolCourse_{{ ConsumerAcronym }}.tblCourse.EntityRemove))))
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewStudentTransport ON ((viewPerson.TblPerson_Id = viewStudentTransport.TblStudent_serviceTblPerson)))
