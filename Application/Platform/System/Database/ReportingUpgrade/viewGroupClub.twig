SELECT
    viewPerson.TblPerson_Id AS TblPerson_Id,
    viewPeopleMetaClub.TblClub_Identifier AS TblClub_Identifier,
    DATE_FORMAT( viewPeopleMetaClub.TblClub_EntryDate, '%d.%m.%Y' ) AS TblClub_EntryDate,
    DATE_FORMAT( viewPeopleMetaClub.TblClub_ExitDate, '%d.%m.%Y' ) AS TblClub_ExitDate,
    viewPeopleMetaClub.TblClub_Remark AS TblClub_Remark
FROM
    (PeoplePerson_{{ ConsumerAcronym }}.viewPerson
    LEFT JOIN PeopleMeta_{{ ConsumerAcronym }}.viewPeopleMetaClub ON ((viewPerson.TblPerson_Id = viewPeopleMetaClub.TblClub_serviceTblPerson)))
