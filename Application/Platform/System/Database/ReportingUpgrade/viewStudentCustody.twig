    (select vP.TblPerson_LastName
        from SettingConsumer_EGE.viewPerson vP
        where vP.TblPerson_Id = TblPerson_S3_Id
    ) TblPerson_LastName_S3,

    (select vP.TblPerson_BirthName
        from SettingConsumer_EGE.viewPerson vP
        where vP.TblPerson_Id = TblPerson_S3_Id
