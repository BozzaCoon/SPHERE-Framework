        where vCA.TblPerson_Id = TblPerson_S2_Id
        and vCA.TblType_Name = 'Hauptadresse'
    ) TblAddress_Nation_S2,

    (select
        group_concat(
            concat(
            vCP.TblType_Name,
            ': ',
            vCP.TblPhone_Number,
            ' - ',
            vCP.TblToPerson_Remark
            ) separator '\n\r'
        ) from SettingConsumer_EGE.viewContactPhone vCP
        where vCP.TblPerson_Id = TblPerson_S2_Id
    ) TblPhone_Number_S2,

    (select
