            ' - ',
            vCM.TblToPerson_Remark
            ) separator '\n\r'
        ) from SettingConsumer_EGE.viewContactMail vCM
        where vCM.TblPerson_Id = TblPerson_S3_Id
    ) TblMail_Address_S3

from SettingConsumer_EGE.viewGroupStudent vGS
    JOIN SettingConsumer_EGE.viewGroup vG ON vG.TblPerson_Id = vGS.TblPerson_Id
where vG.TblGroup_MetaTable = 'STUDENT'