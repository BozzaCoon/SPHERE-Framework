        where vCA.TblPerson_Id = TblPerson_S2_Id
        and vCA.TblType_Name = 'Hauptadresse'
    ) TblCity_Code_S2,

    (select vCA.TblCity_Name
        from SettingConsumer_EGE.viewContactAddress vCA
        where vCA.TblPerson_Id = TblPerson_S2_Id
        and vCA.TblType_Name = 'Hauptadresse'
    ) TblCity_City_S2,

    (select vCA.TblCity_District
