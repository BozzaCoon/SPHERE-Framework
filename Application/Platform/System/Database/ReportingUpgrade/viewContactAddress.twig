SELECT
    vATP.TblToPerson_serviceTblPerson AS TblPerson_Id,
    vATP.TblToPerson_Remark AS TblToPerson_Remark,
    vATP.TblType_Name AS TblType_Name,
    vATP.TblType_Description AS TblType_Description,
    vATP.TblAddress_StreetName AS TblAddress_StreetName,
    vATP.TblAddress_StreetNumber AS TblAddress_StreetNumber,
    vATP.TblAddress_PostOfficeBox AS TblAddress_PostOfficeBox,
    vATP.TblAddress_County AS TblAddress_County,
    vATP.TblAddress_Nation AS TblAddress_Nation,
    vATP.TblCity_Code AS TblCity_Code,
    vATP.TblCity_Name AS TblCity_Name,
    vATP.TblCity_District AS TblCity_District,
    vATP.TblState_Name AS TblState_Name
FROM ContactAddress_{{ ConsumerAcronym }}.viewAddressToPerson vATP
