SELECT
    viewPerson.TblPerson_Id AS TblPerson_Id,
    viewDivisionTeacher.TblDivisionTeacher_Description AS TblDivisionTeacher_Description,
    viewDivisionTeacher.TblLevel_Name AS TblLevel_Name,
    viewDivisionTeacher.TblLevel_Description AS TblLevel_Description,
    viewDivisionTeacher.TblLevel_IsChecked AS TblLevel_IsChecked,
    viewDivisionTeacher.TblDivision_Name AS TblDivision_Name,
    viewDivisionTeacher.TblDivision_Description AS TblDivision_Description,
    viewSchoolType.TblType_Name AS TblType_Name,
    viewSchoolType.TblType_Description AS TblType_Description,
    viewYearPeriod.TblYear_Year AS TblYear_Year,
    viewYearPeriod.TblYear_Description AS TblYear_Description,
    viewYearPeriod.TblPeriod_Name AS TblPeriod_Name,
    viewYearPeriod.TblPeriod_Description AS TblPeriod_Description,
    viewYearPeriod.TblPeriod_FromDate AS TblPeriod_FromDate,
    viewYearPeriod.TblPeriod_ToDate AS TblPeriod_ToDate,
    viewDivisionSubject.TblSubjectGroup_Name AS TblSubjectGroup_Name,
    viewDivisionSubject.TblSubjectGroup_Description AS TblSubjectGroup_Description,
    viewDivisionSubject.TblSubjectGroup_IsAdvancedCourse AS TblSubjectGroup_IsAdvancedCourse,
    viewSubject.TblSubject_Acronym AS TblSubject_Acronym,
    viewSubject.TblSubject_Name AS TblSubject_Name,
    viewSubject.TblSubject_Description AS TblSubject_Description
FROM
    (((((PeoplePerson_{{ ConsumerAcronym }}.viewPerson
    LEFT JOIN EducationLessonDivision_{{ ConsumerAcronym }}.viewDivisionTeacher ON ((viewPerson.TblPerson_Id = viewDivisionTeacher.TblDivisionTeacher_serviceTblPerson)))
    LEFT JOIN EducationSchoolType_{{ ConsumerAcronym }}.viewSchoolType ON ((viewDivisionTeacher.TblLevel_serviceTblType = viewSchoolType.TblType_Id)))
    LEFT JOIN EducationLessonTerm_{{ ConsumerAcronym }}.viewYearPeriod ON ((viewDivisionTeacher.TblDivision_serviceTblYear = viewYearPeriod.TblYearPeriod_tblYear)))
    LEFT JOIN EducationLessonDivision_{{ ConsumerAcronym }}.viewDivisionSubject ON ((viewDivisionTeacher.TblDivision_Id = viewDivisionSubject.TblDivisionSubject_tblDivision)))
    LEFT JOIN EducationLessonSubject_{{ ConsumerAcronym }}.viewSubject ON ((viewDivisionSubject.TblDivisionSubject_serviceTblSubject = viewSubject.TblSubject_Id)))
