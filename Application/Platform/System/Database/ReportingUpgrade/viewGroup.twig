SELECT
    vPGM.TblMember_serviceTblPerson AS TblPerson_Id,
    vPGM.TblGroup_Id AS TblGroup_Id,
    vPGM.TblGroup_Name AS TblGroup_Name,
    vPGM.TblGroup_Description AS TblGroup_Description,
    vPGM.TblGroup_Remark AS TblGroup_Remark,
    vPGM.TblGroup_MetaTable AS TblGroup_MetaTable
FROM PeopleGroup_{{ ConsumerAcronym }}.viewPeopleGroupMember vPGM