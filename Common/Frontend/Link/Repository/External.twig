<a href="{{ ElementPath }}" class="{{ ElementType }}" data-toggle="tooltip" data-container="body"
   title="{{ ElementToolTip }}" rel="external">{{ ElementIcon }} {{ ElementName }}</a>

<script language=javascript>
    //noinspection JSUnresolvedFunction
    executeScript(function()
    {
        {% if RedirectTimeout is not empty %}
            setTimeout("location.href='{{ UrlBase }}{{ RedirectRoute }}'", {{ RedirectTimeout }}000);
        {% endif %}
    });
</script>
