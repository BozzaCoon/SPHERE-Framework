executeScript = function (Callback) {
    Callback();
};

var AjaxNotify{{ NOTIFYHASH }};

jQuery.ajax({
    method:"{{ METHOD }}",
    url:"{{ URL }}",
    data:{{ POST }},
    dataType:"json",
    cache: false,
    beforeSend: function(){
        AjaxNotify{{ NOTIFYHASH }} = $.notify({
            title: 'Daten werden verarbeitet',
            message: '{{ NOTIFYLOADING }}'
        }, {
            showProgressbar: true,
            newest_on_top: true,
            type: 'info',
            delay: 0,
            placement: {
                from: 'top',
                align: 'right'
            },
            template:'<div data-notify="container" class="col-xs-11 col-sm-4 alert alert-{0}" role="alert">' +
    '<button type="button" aria-hidden="true" class="close" data-notify="dismiss">×</button>' +
    '<span data-notify="icon"><span class="loading-indicator-animate"></span>&nbsp;</span>' +
    '<span data-notify="title">{1}</span><br/>' +
    '<div class="progress" data-notify="progressbar" style="height: 3px; margin: 0;">' +
        '<div class="progress-bar progress-bar-{0}" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"></div>' +
        '</div>' +
    '<span data-notify="message">{2}</span>' +
    '<a href="{3}" target="{4}" data-notify="url"></a>' +
'</div>'
        });
        AjaxNotify{{ NOTIFYHASH }}.update({progress: 25});
    },
    error: function (request, status, error) {
        // Parse Error
        var ErrorMessage = '';
        if (request.status === 0) {
            ErrorMessage = ('Not connected.\nPlease verify your network connection.');
        } else if (request.status == 404) {
            ErrorMessage = ('The requested page not found. [404]');
        } else if (request.status == 500) {
            ErrorMessage = ('Internal Server Error [500].');
        } else if (status === 'parsererror') {
            ErrorMessage = ('Requested JSON parse failed.');
        } else if (status === 'timeout') {
            ErrorMessage = ('Time out error.');
        } else if (status === 'abort') {
            ErrorMessage = ('Ajax request aborted.');
        } else {
            ErrorMessage = ('Uncaught Error.\n' + request.responseText);
        }
        // Show Error
        var {{ RESPONSE }} = ''
            + '<span class="text-muted"><small>'
            + 'Url: {{ URL_BASE }}'
            + '<br/>'
            + 'Data: {{ POST }}'
            + '</small></span>'
        jQuery.notify({ title: ErrorMessage, message: {{ RESPONSE }} }, {
            newest_on_top: true,
            type: 'danger',
            placement: { from: "top", align: "right" },
            delay: 0,
            animate: { enter: 'animated fadeInRight', exit: 'animated fadeOutRight' },
template:'<div data-notify="container" class="col-xs-11 col-sm-8 alert alert-{0}" role="alert">' +
    '<button type="button" aria-hidden="true" class="close" data-notify="dismiss">×</button>' +
    '<span data-notify="icon"></span>' +
    '<span data-notify="title">{1}</span><br/>' +
    '<div class="progress" data-notify="progressbar" style="height: 3px; margin: 0;">' +
        '<div class="progress-bar progress-bar-{0}" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"></div>' +
        '</div>' +
    '<span data-notify="message">{2}</span>' +
    '<a href="{3}" target="{4}" data-notify="url"></a>' +
    '</div>'
        });
    },
    success: function({{ RESPONSE }}){
        AjaxNotify{{ NOTIFYHASH }}.update({progress: 75});
        {% for Receiver in ReceiverList %}
            {{ Receiver.getHandler }}
        {% endfor %}
        {{ NOTIFY }}
    }
}){{ CALLBACK }};